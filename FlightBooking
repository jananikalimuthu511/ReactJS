export default function FlightBooking() {
  const bookFlight = () => {
    const name = prompt("Enter passenger name");
    if (confirm("Confirm booking?")) {
      alert(`Flight booked for ${name}`);
    }
  };

  return (
    <>
      <h1>Flight Booking</h1>
      <button onClick={bookFlight}>Book Ticket</button>
    </>
  );
}
